					pinctrl-names = "default";
					pinctrl-0 = <0x10 0x11>;
					fsl,uart-has-rtscts;
					fsl,dte-mode;
				};

				esai@02024000 {
					compatible = "fsl,imx6ull-esai";
					reg = <0x2024000 0x4000>;
					interrupts = <0x0 0x33 0x4>;
					clocks = <0x1 0xe6 0x1 0xe5 0x1 0xe4 0x1 0xe6 0x1 0xbb>;
					clock-names = "core", "mem", "extal", "fsys", "spba";
					dmas = <0x9 0x0 0x15 0x0 0x9 0x2f 0x15 0x0>;
