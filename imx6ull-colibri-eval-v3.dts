					dmas = <0x9 0x3 0x7 0x1 0x9 0x4 0x7 0x2>;
					dma-names = "rx", "tx";
					status = "okay";
					fsl,spi-num-chipselects = <0x1>;
					cs-gpios = <0xa 0x1a 0x0>;
					pinctrl-names = "default";
					pinctrl-0 = <0xb 0xc>;

					mcp251x@0 {
						compatible = "microchip,mcp2515";
						pinctrl-names = "default";
						pinctrl-0 = <0xd>;
						reg = <0x0>;
						clocks = <0xe>;
						interrupt-parent = <0xf>;
						interrupts = <0x4 0x2>;
						spi-max-frequency = <0x989680>;
