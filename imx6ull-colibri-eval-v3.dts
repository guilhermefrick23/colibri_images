				ecspi@02010000 {
					#address-cells = <0x1>;
					#size-cells = <0x0>;
					compatible = "fsl,imx6ul-ecspi", "fsl,imx51-ecspi";
					reg = <0x2010000 0x4000>;
					interrupts = <0x0 0x21 0x4>;
					clocks = <0x1 0x8d 0x1 0x8d>;
					clock-names = "ipg", "per";
					dmas = <0x9 0x7 0x7 0x1 0x9 0x8 0x7 0x2>;
